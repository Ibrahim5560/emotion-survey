<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.9.xsd">
    <!--
        Added the constraints for entity Messages.
    -->
    <changeSet id="20210201054608-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="center_id"
                                 baseTableName="messages"
                                 constraintName="fk_messages_center_id"
                                 referencedColumnNames="id"
                                 referencedTableName="center"/>

        <addForeignKeyConstraint baseColumnNames="system_id"
                                 baseTableName="messages"
                                 constraintName="fk_messages_system_id"
                                 referencedColumnNames="id"
                                 referencedTableName="system"/>

        <addForeignKeyConstraint baseColumnNames="system_services_id"
                                 baseTableName="messages"
                                 constraintName="fk_messages_system_services_id"
                                 referencedColumnNames="id"
                                 referencedTableName="system_services"/>

        <addForeignKeyConstraint baseColumnNames="users_id"
                                 baseTableName="messages"
                                 constraintName="fk_messages_users_id"
                                 referencedColumnNames="id"
                                 referencedTableName="users"/>

    </changeSet>
</databaseChangeLog>
